generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TrainStation {
  id               Int    @id // SBB BPUIC
  description      String
  descriptionShort String
  lon              Float // auf 15 Kommastellen gerundet
  lat              Float // auf 15 Kommastellen gerundet
}
